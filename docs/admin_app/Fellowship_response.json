"{\"code\":\"// begin MANDATORY imports (always generated EXACTLY)\\nimport React from 'react';\\nimport { List, FunctionField, Datagrid, TextField, DateField, NumberField } from 'react-admin';\\nimport { ReferenceField, ReferenceManyField } from 'react-admin';\\nimport { TabbedShowLayout, Tab, SimpleShowLayout, TextInput, NumberInput, DateTimeInput } from 'react-admin';\\nimport { ReferenceInput, SelectInput, SimpleForm, Show, Edit, Create } from 'react-admin';\\nimport { Filter, Pagination, BooleanField, BooleanInput, Labeled } from 'react-admin'; \\nimport { EditButton, DeleteButton, CreateButton } from 'react-admin';\\nimport { Grid, Typography, Box, Divider, Button } from '@mui/material';\\nimport { useRecordContext, useRedirect, Link, required } from 'react-admin';\\nimport AddIcon from '@mui/icons-material/Add';\\n// end mandatory imports\\n\\n// Filter for Fellowship List\\nconst FellowshipFilter = (props) => (\\n    <Filter {...props}>\\n        <TextInput label=\\\"Search\\\" source=\\\"name\\\" alwaysOn />\\n    </Filter>\\n);\\n\\n// Fellowship List\\nexport const FellowshipList = (props) => {\\n    return (\\n        <List filters={<FellowshipFilter />} {...props} sort={{ field: 'name', order: 'ASC' }} pagination={<Pagination rowsPerPageOptions={[5, 10, 25]} showFirstLastButtons />}>\\n            <Datagrid rowClick=\\\"show\\\">\\n                <TextField source=\\\"name\\\" label=\\\"Name\\\" />\\n                <TextField source=\\\"full_name\\\" label=\\\"Full Name\\\" />\\n                <TextField source=\\\"website\\\" label=\\\"Website\\\" />\\n            </Datagrid>\\n        </List>\\n    );\\n};\\n\\n// Fellowship Show\\nexport const FellowshipShow = (props) => {\\n    return (\\n        <Show {...props}>\\n            <SimpleShowLayout>\\n                <Box sx={{ mb: 3 }}>\\n                    <Typography variant=\\\"h5\\\" component=\\\"h2\\\" sx={{ mb: 2, fontWeight: 'bold' }}>\\n                        Fellowship Information\\n                    </Typography>\\n                    <Grid container spacing={3} sx={{ mb: 2 }}>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <Labeled label=\\\"Name\\\">\\n                                    <TextField source=\\\"name\\\" />\\n                                </Labeled>\\n                            </Box>\\n                        </Grid>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <Labeled label=\\\"Full Name\\\">\\n                                    <TextField source=\\\"full_name\\\" />\\n                                </Labeled>\\n                            </Box>\\n                        </Grid>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <Labeled label=\\\"Website\\\">\\n                                    <TextField source=\\\"website\\\" />\\n                                </Labeled>\\n                            </Box>\\n                        </Grid>\\n                    </Grid>\\n                    <Divider sx={{ my: 2 }} />\\n                </Box>\\n            </SimpleShowLayout>\\n            <TabbedShowLayout>\\n                <Tab label=\\\"Cards\\\">\\n                    <ReferenceManyField reference=\\\"Card\\\" target=\\\"fellowship_name\\\" addLabel={false} pagination={<Pagination />}>  \\n                        <Datagrid rowClick=\\\"show\\\">\\n                            <TextField source=\\\"id\\\" label=\\\"Card ID\\\" />\\n                            <TextField source=\\\"fellowship_name\\\" label=\\\"Fellowship Name\\\" />\\n                            <TextField source=\\\"circle_text\\\" label=\\\"Circle Text\\\" />\\n                            <BooleanField source=\\\"is_active\\\" label=\\\"Active\\\" />\\n                            <EditButton />\\n                            <DeleteButton />\\n                        </Datagrid>\\n                    </ReferenceManyField>\\n                    <AddCardButton />\\n                </Tab>\\n                <Tab label=\\\"Tags\\\">\\n                    <ReferenceManyField reference=\\\"Tag\\\" target=\\\"fellowship_name\\\" addLabel={false} pagination={<Pagination />}>  \\n                        <Datagrid rowClick=\\\"show\\\">\\n                            <TextField source=\\\"id\\\" label=\\\"Tag ID\\\" />\\n                            <TextField source=\\\"tag_name\\\" label=\\\"Tag Name\\\" />\\n                            <EditButton />\\n                            <DeleteButton />\\n                        </Datagrid>\\n                    </ReferenceManyField>\\n                </Tab>\\n                <Tab label=\\\"Users\\\">\\n                    <ReferenceManyField reference=\\\"User\\\" target=\\\"fellowship_name\\\" addLabel={false} pagination={<Pagination />}>  \\n                        <Datagrid rowClick=\\\"show\\\">\\n                            <TextField source=\\\"id\\\" label=\\\"User ID\\\" />\\n                            <TextField source=\\\"name\\\" label=\\\"Name\\\" />\\n                            <TextField source=\\\"email\\\" label=\\\"Email\\\" />\\n                            <EditButton />\\n                            <DeleteButton />\\n                        </Datagrid>\\n                    </ReferenceManyField>\\n                </Tab>\\n            </TabbedShowLayout>\\n        </Show>\\n    );\\n};\\n\\n// Custom Add Card Button\\nconst AddCardButton = () => {\\n    const record = useRecordContext();\\n    const redirect = useRedirect();\\n    \\n    const handleClick = () => {\\n        redirect(`/Card/create?source=${encodeURIComponent(JSON.stringify({ fellowship_name: record?.name }))}`);\\n    };\\n\\n    return (\\n        <Button\\n            variant=\\\"contained\\\"\\n            color=\\\"primary\\\"\\n            startIcon={<AddIcon />}\\n            onClick={handleClick}\\n            sx={{ mt: 2 }}\\n        >\\n            Add New Card\\n        </Button>\\n    );\\n};\\n\\n// Fellowship Create\\nexport const FellowshipCreate = (props) => {\\n    return (\\n        <Create {...props}>\\n            <SimpleForm>\\n                <Box sx={{ mb: 3 }}>\\n                    <Typography variant=\\\"h5\\\" component=\\\"h2\\\" sx={{ mb: 2, fontWeight: 'bold' }}>\\n                        Create New Fellowship\\n                    </Typography>\\n                    <Grid container spacing={3} sx={{ mb: 2 }}>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <TextInput source=\\\"name\\\" label=\\\"Name\\\" fullWidth validate={required()} />\\n                            </Box>\\n                        </Grid>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <TextInput source=\\\"full_name\\\" label=\\\"Full Name\\\" fullWidth />\\n                            </Box>\\n                        </Grid>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <TextInput source=\\\"website\\\" label=\\\"Website\\\" fullWidth />\\n                            </Box>\\n                        </Grid>\\n                    </Grid>\\n                </Box>\\n            </SimpleForm>\\n        </Create>\\n    );\\n};\\n\\n// Fellowship Edit\\nexport const FellowshipEdit = (props) => {\\n    return (\\n        <Edit {...props} redirect={false}>\\n            <SimpleForm>\\n                <Box sx={{ mb: 3 }}>\\n                    <Typography variant=\\\"h5\\\" component=\\\"h2\\\" sx={{ mb: 2, fontWeight: 'bold' }}>\\n                        Edit Fellowship\\n                    </Typography>\\n                    <Grid container spacing={3} sx={{ mb: 2 }}>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <TextInput source=\\\"name\\\" label=\\\"Name\\\" fullWidth validate={required()} />\\n                            </Box>\\n                        </Grid>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <TextInput source=\\\"full_name\\\" label=\\\"Full Name\\\" fullWidth />\\n                            </Box>\\n                        </Grid>\\n                        <Grid item xs={12} sm={6} md={4}>\\n                            <Box sx={{ p: 1 }}>\\n                                <TextInput source=\\\"website\\\" label=\\\"Website\\\" fullWidth />\\n                            </Box>\\n                        </Grid>\\n                    </Grid>\\n                </Box>\\n            </SimpleForm>\\n        </Edit>\\n    );\\n};\\n\\nexport default {\\n    list: FellowshipList,\\n    show: FellowshipShow,\\n    create: FellowshipCreate,\\n    edit: FellowshipEdit,\\n};\"}"