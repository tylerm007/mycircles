                <TextField source=\\\"id\\\" label=\\\"ID\\\" />\\n                <ReferenceField source=\\\"card_id\\\" reference=\\\"Card\\\" label=\\\"Card\\\">\\n                    <TextField source=\\\"fellowship_name\\\" />\\n                </ReferenceField>\\n                <ReferenceField source=\\\"tag_id\\\" reference=\\\"Tag\\\" label=\\\"Tag\\\">\\n                    <TextField source=\\\"tag_name\\\" />\\n                </ReferenceField>\\n            </SimpleShowLayout>\\n        </Show>\\n    );\\n};\\n\\n// CardTag Create\\nexport const CardTagCreate = (props) => {\\n    return (\\n        <Create {...props}>\\n            <SimpleForm>\\n                <Typography variant=\\\"h5\\\" component=\\\"h2\\\" sx={{ mb: 2, fontWeight: 'bold' }}>\\n                    Create New Card Tag\\n                </Typography>\\n                <Grid container spacing={3} sx={{ mb: 2 }}>\\n                    <Grid item xs={12} sm={6} md={4}>\\n                        <Box sx={{ p: 1 }}>\\n                            <ReferenceInput source=\\\"card_id\\\" reference=\\\"Card\\\" fullWidth>\\n                                <SelectInput optionText=\\\"fellowship_name\\\" validate={required()} />\\n                            </ReferenceInput>\\n                        </Box>\\n                    </Grid>\\n                    <Grid item xs={12} sm={6} md={4}>\\n                        <Box sx={{ p: 1 }}>\\n                            <ReferenceInput source=\\\"tag_id\\\" reference=\\\"Tag\\\" fullWidth>\\n                                <SelectInput optionText=\\\"tag_name\\\" validate={required()} />\\n                            </ReferenceInput>\\n                        </Box>\\n                    </Grid>\\n                </Grid>\\n            </SimpleForm>\\n        </Create>\\n    );\\n};\\n\\n// CardTag Edit\\nexport const CardTagEdit = (props) => {\\n    return (\\n        <Edit {...props} redirect={false}>\\n            <SimpleForm>\\n                <Typography variant=\\\"h5\\\" component=\\\"h2\\\" sx={{ mb: 2, fontWeight: 'bold' }}>\\n                    Edit Card Tag\\n                </Typography>\\n                <Grid container spacing={3} sx={{ mb: 2 }}>\\n                    <Grid item xs={12} sm={6} md={4}>\\n                        <Box sx={{ p: 1 }}>\\n                            <ReferenceInput source=\\\"card_id\\\" reference=\\\"Card\\\" fullWidth>\\n                                <SelectInput optionText=\\\"fellowship_name\\\" validate={required()} />\\n                            </ReferenceInput>\\n                        </Box>\\n                    </Grid>\\n                    <Grid item xs={12} sm={6} md={4}>\\n                        <Box sx={{ p: 1 }}>\\n                            <ReferenceInput source=\\\"tag_id\\\" reference=\\\"Tag\\\" fullWidth>\\n                                <SelectInput optionText=\\\"tag_name\\\" validate={required()} />\\n                            </ReferenceInput>\\n                        </Box>\\n                    </Grid>\\n                </Grid>\\n            </SimpleForm>\\n        </Edit>\\n    );\\n};\\n\\nexport default {\\n    list: CardTagList,\\n    show: CardTagShow,\\n    create: CardTagCreate,\\n    edit: CardTagEdit\\n};\\n\"}"